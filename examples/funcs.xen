include io;

fn add(a, b) => a + b
fn inv_mod(a, b) => -a % -b

fn print_type_str(t) {
    var type_str = typeof(t);
    io.println("Type: ", type_str);
}

var add_result = add(3, 4);
var inv_result = inv_mod(7, 4);

io.println("Add result: ", add_result);
io.println("Inv result: ", inv_result);
print_type_str(45);
